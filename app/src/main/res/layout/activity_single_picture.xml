<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.user1.volleyballmanager20.SinglePictureActivity">

    <Button
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:id="@+id/btn_erase"
        android:background="@android:drawable/ic_delete"/>
    <Button
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:id="@+id/btn_load_on_tactics"
        android:layout_alignParentBottom="true"
        android:background="@android:drawable/ic_menu_set_as"/>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@id/btn_load_on_tactics"
        android:id="@+id/single_picture"/>

</RelativeLayout>
